<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exp.androidtv">

  <application
      android:name=".AndroidTVApplication"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme.NoActionBar">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".ui.list.GVListActivity" />
    <activity android:name=".ui.list.GHListActivity" />
    <activity android:name=".ui.list.RHListActivity" />
    <activity android:name=".ui.list.RVListActivity" />
    <activity android:name=".ui.list.RGListActivity" />
    <activity android:name=".ui.list.GNListActivity" />
    <activity android:name=".ui.list.RNListActivity" />
    <activity android:name=".ui.list.RSListActivity" />
    <activity android:name=".ui.page.OutFrameActivity" />

    <meta-data
        android:name="design_width_in_dp"
        android:value="1280" />
    <meta-data
        android:name="design_height_in_dp"
        android:value="720" />
  </application>

</manifest>